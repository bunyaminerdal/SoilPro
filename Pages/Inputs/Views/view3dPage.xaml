<Page x:Class="SoilPro.Pages.Inputs.Views.View3dPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SoilPro.Pages.Inputs.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="view3dPage">

    <Grid Background="White" MouseWheel="viewport3d_main_MouseWheel">
        <Label x:Name="scroll_3dview" Content="adfasdf"></Label>
        <Button x:Name="Position_Reset_bttn" Width="50" Height="50" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="10" Click="Position_Reset_bttn_Click"></Button>
        <ContentControl Name="contentControl2">
            <Viewport3D x:Name="viewport3d_main" ClipToBounds="True" Width="Auto" Height="Auto" />

        </ContentControl>
    </Grid>
</Page>
